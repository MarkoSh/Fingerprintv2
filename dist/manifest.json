{
	"manifest_version": 3,
	"name": "Fingerprint v2.0.0",
	"version": "2.0.0",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"main.js"
			],
			"run_at": "document_end"
		}
	],
	"background": {
		"service_worker": "main.js"
	},
	"action": {
		"default_title": "Fingerprint v2.0.0",
		"default_icon": "icon.png"
	},
	"host_permissions": ["<all_urls>"],
	"permissions": [
		"contextMenus",
		"clipboardRead",
		"clipboardWrite",
		"storage",
		"cookies"
	],
	"icons": {
		"16": "icon.png",
		"32": "icon.png"
	},
	"web_accessible_resources": [
		{
			"matches": [
				"<all_urls>"
			],
			"resources": [
				"manifest.json",

				"main.js"
			]
		}
	]
}