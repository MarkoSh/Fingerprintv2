{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "COPY",
			"type": "shell",
			"group": "build",
			"command": "cp",
			"args": [
				"src/*.png",
				"src/*.json",
				"dist/"
			]
		},
		{
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "tsc: build - tsconfig.json",
			"dependsOn": [
				"COPY"
			]
		}
	]
}